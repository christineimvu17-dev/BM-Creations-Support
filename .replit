entrypoint = "main.py"
modules = ["python-3.10", "nix"]

hidden = [".pythonlibs"]

[nix]
channel = "stable-25_05"
packages = ["postgresql"]

[deployment]
run = ["python", "main.py"]
deploymentTarget = "gce"
ignorePorts = true

[agent]
expertMode = true
integrations = ["discord:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Discord Bot"

[[workflows.workflow]]
name = "Discord Bot"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33587
externalPort = 3000
